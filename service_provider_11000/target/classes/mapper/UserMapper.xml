<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ynu.edu.mapper.UserMapper">

    <update id="updateImg">
        update user set userImg = #{userImg}
        where userId = #{userId}
    </update>

</mapper>
